# MongoDB connection settings
# Copy this file to `.env` and fill in values.

# Example for MongoDB Atlas:
# MONGODB_URI=mongodb+srv://<user>:<password>@<cluster-host>/<optional-default-db>

MONGODB_URI=
MONGODB_DB=smart_drain
MONGODB_COLLECTION=readings

# Serial settings (optional; used if you run Serial_reader with no CLI args)
SERIAL_PORT=COM3
SERIAL_BAUD=9600
